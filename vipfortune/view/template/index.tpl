				break;
				case 3:
				$(".starbox3").show();
				break;
				case 4:
				$(".starbox4").show();
				break;
				case 5:
				$(".starbox5").show();
				break;
				case 6:
				$(".starbox6").show();
				break;
				case 7:
				$(".starbox7").show();
				break;
				case 8:
