		//移除星星对应的奖项提示div
		function CMD_tipStarRemove()
		{
			console.log("星星弹窗提示移除");
			$(".starbox").hide();
		}
		
		//弹出星星对应的提示div
		function CMD_tipStar(val)
		{
			console.log("添加星星弹窗提示,星星等级:"+val);
			switch(val)
			{
				case 1:
				$(".starbox1").show();
				break;
