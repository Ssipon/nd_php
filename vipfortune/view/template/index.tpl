		//设置转盘初始化数据/重置转盘
		function HandleSetZhuanPanInit()
		{
			$.get( '?r=/index/initDail',
				   function(data) {
						$('#flash').flash(function () {
							this.API_setZhuanPanInitData(data);
						});
					} 
			); 
		}
		
		//转盘升级请求
		//val 当前等级  1:青铜 2:白银 3:黄金 4:钻石
		function CMD_lvUp(val)
		{
			$.get( '?r=/index/levelUp',
					   function(data) {
						var dataJson = JSON.parse(data);
						HandleSetZhuanPanInit();
						if ( typeof ( dataJson.isLogin ) != 'undefined' && false == dataJson.isLogin ) {
								showlogBox(); 
								return;
							}  
							HandleSetZhuanPanInit(); 
							showMsgBox(dataJson.fail_info);
			} );
		}
		
		//转盘抽奖请求
		function CMD_getZhuanPanReward()
		{
				$.ajax({
					url  : '?r=/lottery/exection',
					type : 'POST',
					dataType : 'text',
					beforeSend : function() {
							$('#loading').show();
					   },
					error : function(){
						$('#loading').hide();
						showMsgBox('Abnormal network cause! Failed to submit. Please submit again later.');
					}, 
					success :function(data) {
					$('#loading').hide();
				    HandleSetZhuanPanInit();
					var dataJson = JSON.parse(data);
					if ( typeof ( dataJson.isLogin ) != 'undefined' && false == dataJson.isLogin ) {
							showlogBox();
							return;
						} else if ( typeof ( dataJson.success ) != 'undefined' && 1 == dataJson.success ) {
							$('#flash').flash(function () {
								this.API_setZhuanPanRewardData(data);
							});
							$('#totalCredit').text(dataJson.totalCredit);
							$('#remainCredit').text(dataJson.remainCredit);
							$('#uesdCredit').text(dataJson.uesdCredit);
							return ;
						}
						showMsgBox(dataJson.fail_info);
					}} );
		}
		
	 	//提示小缩略图转盘的奖项
		//val_pos 转盘缩略图位置  1-3
		//val_type 转盘类型  1:青铜 2:白银 3:黄金 4:钻石
		function CMD_tipZhuanPan(val_pos,val_type)
		{
			console.log("提示转盘的奖项"+"转盘位置:"+val_pos+"转盘类型:"+val_type);
			switch(val_type)
			{
				case 1:
				$(".wheelbox1").show();
				break;
				case 2:
				$(".wheelbox2").show();
				break;
				case 3:
				$(".wheelbox3").show();
				break;
				case 4:
				$(".wheelbox4").show();
				break;
			}
		}
		
		//鼠标移开对应的缩略图时被调用
		//隐藏小缩略图的奖项
		function CMD_tipZhuanPanRemove()
		{
			console.log("隐藏转盘奖项");
			$(".wheelbox").hide();
		}
		
		//展示抽中的对应的转盘和奖项
		function CMD_showZhuanPanReward(type,data)
		{
			showMsgBox("Congratulations！You gained "+data.name+" as reward. Please claim your reward from the Prize Officer (Market 185*170)." );
		}
		
